<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.fox.dao.two.StudentDao">
 
    <select id="getName" resultType="String">
        SELECT [STUDENT_NAME]
  			FROM [activiti].[dbo].[STUDENT] where STUDENT_ID = 2;
    </select>
</mapper>